{
  "name": "",
  "version": "0.1.0",
  "main": "",
  "private": true,
  "scripts": {
    "dev": "",
    "build": "",
    "lint": "eslint \"{,src/**/*}.{mjs,js,ts,jsx,tsx,vue}\"",
    "stylelint": "stylelint \"{,src/**/*}.{css,scss,less,styl,vue}\"",
    "prettier": "prettier --write \"{,src/**/*}.{js,ts,jsx,tsx,json,css,less,scss,styl,vue}\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{css,less,sass,scss}": [
      "prettier --write",
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": "^12.0.0 || >= 14.0.0"
  }
}
